{
  "name": "my-budget",
  "version": "0.3.13",
  "scripts": {
    "bump": "standard-version --skip.tag",
    "release": "git flow release start `sh scripts/nextver.sh`",
    "publish": "git flow release finish `sh scripts/ver.sh`",
    "dev": "next  dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "eslint": "eslint \"src/**/*.{j,t}s{,x}\" --fix",
    "check:ts": "tsc --extendedDiagnostics",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs-build",
    "loki:test": "loki test",
    "loki:build": "loki  --requireReference --reactUri file:./storybook-static",
    "loki:approve": "loki approve",
    "loki:update": "loki update",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "lint-staged": {
    "!(*min).{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": "20",
    "npm": "10.2.4"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.9",
    "@hookform/resolvers": "^3.3.2",
    "@mdx-js/loader": "^3.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/bundle-analyzer": "^13.5.6",
    "@prisma/client": "^5.7.0",
    "@salutejs/plasma-icons": "1.173.0",
    "@salutejs/plasma-tokens": "^1.55.0",
    "@salutejs/plasma-tokens-b2b": "^1.22.0",
    "@salutejs/plasma-tokens-b2c": "^0.32.0",
    "@salutejs/plasma-tokens-web": "^1.36.0",
    "@salutejs/plasma-ui": "^1.223.0",
    "@salutejs/plasma-web": "^1.250.0",
    "@salutejs/use-virtual": "2.0.0",
    "@types/node": "20.11.28",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.5",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "encoding": "^0.1.13",
    "eslint-config-next": "13.3.4",
    "next": "14.0.2",
    "next-auth": "^4.24.5",
    "next-pwa": "^5.6.0",
    "node-fetch": "^3.3.2",
    "nookies": "^2.5.2",
    "react": "18.2.0",
    "react-avatar": "^5.0.3",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "18.2.0",
    "react-google-button": "^0.7.2",
    "react-hook-form": "^7.48.2",
    "react-swipeable-list": "^1.9.3",
    "react-toastify": "^9.1.3",
    "styled-components": "^5.3.11",
    "swr": "^2.2.4",
    "typescript": "5.1.3",
    "uuid": "^9.0.1",
    "yup": "^1.3.2",
    "yup-locale-ru": "^1.0.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.3",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^8.0.0",
    "@storybook/nextjs": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/test": "^7.6.4",
    "@testing-library/react": "^14.2.1",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.0",
    "chromatic": "^11.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "loki": "^0.33.1",
    "next": "^14.1.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.0",
    "sass": "^1.69.5",
    "standard-version": "^9.5.0",
    "storybook": "^7.6.4",
    "vitest": "^1.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    }
  }
}
